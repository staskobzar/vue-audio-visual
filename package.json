{
  "name": "vue-audio-visual",
  "version": "3.0.1",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vue-audio-visual.umd.cjs",
  "module": "./dist/vue-audio-visual.js",
  "exports": {
    ".": {
      "import": "./dist/vue-audio-visual.js",
      "require": "./dist/vue-audio-visual.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-demo build-dist",
    "preview": "vite preview --port 4173",
    "test:unit": "vitest --environment jsdom",
    "test:ci": "vitest --environment jsdom --run --coverage",
    "build-demo": "vite build --base=/vue-audio-visual/",
    "build-dist": "vite build --config vite.config.dist.ts",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore"
  },
  "dependencies": {
    "@vueuse/core": "^9.1.0",
    "vue": "^3.2.37"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.4",
    "@types/jsdom": "^20.0.0",
    "@types/node": "^18.7.3",
    "@vitejs/plugin-basic-ssl": "^0.1.2",
    "@vitejs/plugin-vue": "^3.0.1",
    "@vue/eslint-config-typescript": "^11.0.0",
    "@vue/test-utils": "^2.0.2",
    "@vue/tsconfig": "^0.1.3",
    "c8": "^7.12.0",
    "eslint": "^8.21.0",
    "eslint-plugin-vue": "^9.3.0",
    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "typescript": "~4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.21.0",
    "vue-tsc": "^0.40.1"
  }
}
